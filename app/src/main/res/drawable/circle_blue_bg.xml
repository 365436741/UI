<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 外圆 -->
    <item>
        <shape android:shape="oval" >
            <solid android:color="#ffffffff" />

            <stroke
                android:width="1px"
                android:color="#90000000" />
        </shape>
    </item>

    <!-- 内圆 -->
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="oval" >
            <solid android:color="#ff0000ff" />
        </shape>
    </item>

</layer-list>